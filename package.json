  "scripts": {
    "setup": "yarn install && yarn native && yarn migrate:latest",
    "start": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "native": "electron-builder install-app-deps",
    "migrate:make": "yarn knex migrate:make",
    "migrate:latest": "yarn knex migrate:latest",
    "migrate:rollback": "yarn knex migrate:rollback",
    "nuke:db": "rm -f .local/default.sqlite3 && yarn migrate:latest"
  },
